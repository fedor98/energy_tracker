{
    "$schema": "https://opencode.ai/config.json",
    "provider": {
        "llama.cpp": {
            "npm": "@ai-sdk/openai-compatible",
            "name": "llama-cpp-server",
            "options": {
                "baseURL": "http://192.168.178.159:8786/v1"
            },
            "models": {
                "qwen3-coder:a3b": {
                    "name": "Qwen3-Coder: a3b-30b (local)",
                    "limit": {
                        "context": 128000,
                        "output": 65536
                    }
                }
            }
        },
        "ollama-server": {
            "npm": "@ai-sdk/openai-compatible",
            "name": "Ollama-server",
            "options": {
                "baseURL": "http://192.168.178.159:11434/v1"
            },
            "models": {
                "qwen3-coder:30b": {
                    "name": "qwen3-coder"
                }
            }
        },
        "azurefoundry": {
            "npm": "@ai-sdk/openai-compatible",
            "name": "AzureFoundry-personal",
            "options": {
                "baseURL": "https://{env:AZURE_FOUNDRY_RESOURCE}.services.ai.azure.com/openai/v1/",
                "apiKey": "{env:AZURE_API_KEY}"
            },
            "models": {
                "DeepSeek-V3.2": {
                    "name": "DeepSeek-V3.2",
                    "interleaved": {
                        "field": "reasoning_content"
                    }
                },
                "Kimi-K2-Thinking": {
                    "name": "Kimi-K2-Thinking",
                    "interleaved": {
                        "field": "reasoning_content"
                    }
                }
            }
        }
    }
}